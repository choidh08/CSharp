                                        </Border>
                                        <Border Grid.Row="3" Grid.Column="2" Style="{StaticResource Border_Line_2_BR}">
                                            <TextBlock Text="{x:Static localization:Resource.WithdrawDepositWithdraw_1_12}" FontSize="12" Foreground="#666" VerticalAlignment="Center" Margin="5,0,0,0"/>
                                        </Border>
                                        <Border Grid.Row="3" Grid.Column="3" Style="{StaticResource Border_Line_1_B}">
                                            <Grid HorizontalAlignment="Right">
                                                <StackPanel Orientation="Horizontal" Margin="0,0,5,0" Visibility="{Binding EmailRequest}">
                                                    <dxe:TextEdit Text="" FontSize="13" Foreground="#333" Width="70" IsEnabled="{Binding EmailAuthCodeEnabled}" HorizontalContentAlignment="Right" VerticalAlignment="Center" Margin="0,0,5,0"/>
                                                    <Button Width="108" Height="26" Cursor="Hand" HorizontalAlignment="Center" Command="{Binding CmdEmailRequestCommand}">
                                                        <Button.Style>
                                                            <Style TargetType="{x:Type Button}">
                                                                <Setter Property="Template">
                                                                    <Setter.Value>
                                                                        <ControlTemplate TargetType="{x:Type Button}">
                                                                            <Grid>
                                                                                <Image Name="btnimg" Source="{Binding btn_certnum_send, TargetNullValue={x:Null}}"/>
                                                                            </Grid>
                                                                            <ControlTemplate.Triggers>
                                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                                    <Setter Property="Source" TargetName="btnimg" Value="{Binding btn_certnum_send_on}"/>
                                                                                </Trigger>
