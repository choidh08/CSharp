                                                                        </ControlTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </Style>
                                                        </Button.Style>
                                                    </Button>
                                                </StackPanel>
                                                <StackPanel Orientation="Horizontal" Margin="0,0,5,0" Visibility="{Binding EmailComplete}">
                                                    <TextBlock Text="{x:Static localization:Resource.IP_Registration_4_11}" FontSize="13" Foreground="#333" VerticalAlignment="Center" Margin="5,0,0,0" />
                                                </StackPanel>
                                            </Grid>
                                        </Border>

                                        <Border Grid.Row="4" Grid.Column="0" Style="{StaticResource Border_Line_2_BR}">
                                            <TextBlock Text="{x:Static localization:Resource.WithdrawDepositWithdraw_1_14}" FontSize="12" Foreground="#666" VerticalAlignment="Center" Margin="5,0,0,0"/>
                                        </Border>
                                        <Border Grid.Row="4" Grid.Column="1" Style="{StaticResource Border_Line_1_BR}">
                                            <TextBlock Text="{Binding accountNo}" FontSize="12" Foreground="#333" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0,0,12,0"/>
                                            <!--<dxe:TextEdit Width="168" Height="26" Text="{Binding accountNo}" FontSize="12" Foreground="#333" HorizontalAlignment="Right" VerticalAlignment="Center"/>-->
                                        </Border>
                                        <Border Grid.Row="4" Grid.Column="2" Style="{StaticResource Border_Line_2_BR}">
                                            <TextBlock Text="{x:Static localization:Resource.WithdrawDepositWithdraw_1_15}" Visibility="{Binding OtpVisible}" FontSize="12" Foreground="#666" VerticalAlignment="Center" Margin="5,0,0,0"/>
                                        </Border>
                                        <Border Grid.Row="4" Grid.Column="3" Style="{StaticResource Border_Line_1_B}">
                                            <Grid Visibility="{Binding OtpVisible}">
                                                <StackPanel VerticalAlignment="Center" Visibility="{Binding OtpRequest}">
                                                    <TextBlock Text="{x:Static localization:Resource.WithdrawDepositWithdraw_Common_9}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" Foreground="#333" Margin="0,0,5,0"/>
                                                </StackPanel>
                                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0,0,5,0" Visibility="{Binding OtpConfirm}">
                                                    <dxe:TextEdit Text="{Binding OtpSerialNumber}" Width="100" Height="26" HorizontalContentAlignment="Right" HorizontalAlignment="Right"/>
                                                    <Button Width="65" Height="25" Cursor="Hand" HorizontalAlignment="Center" Command="{Binding CmdOtpConfirmCommand}" Margin="5,0,0,0">
                                                        <Button.Style>
                                                            <Style TargetType="{x:Type Button}">
                                                                <Setter Property="Template">
                                                                    <Setter.Value>
                                                                        <ControlTemplate TargetType="{x:Type Button}">
                                                                            <Grid>
                                                                                <Image Name="btnimg" Source="{Binding btn_cert_confirm, TargetNullValue={x:Null}}"/>
                                                                            </Grid>
                                                                            <ControlTemplate.Triggers>
                                                                                <Trigger Property="IsMouseOver" Value="True">
