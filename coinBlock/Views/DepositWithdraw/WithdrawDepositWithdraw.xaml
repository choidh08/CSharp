                                                    <dxmvvm:Interaction.Behaviors>
                                                        <dxmvvm:EventToCommand EventName="PreviewTextInput" Command="{Binding CmdStrNonInputCommand}" PassEventArgsToCommand="True" />
                                                    </dxmvvm:Interaction.Behaviors>
                                                </dxe:TextEdit>
                                                <Grid HorizontalAlignment="Right" Margin="5,0,0,0">
                                                    <StackPanel Orientation="Horizontal" Margin="0,0,5,0" Visibility="{Binding AccountSms.SmsRequest}">
                                                        <dxe:TextEdit Text="" FontSize="13" Foreground="#333" Width="70" Height="23" IsEnabled="{Binding AccountSms.SmsAuthCodeEnabled}" HorizontalContentAlignment="Right" VerticalAlignment="Center" Margin="5,0,5,0" Visibility="Collapsed"/>
                                                        <Button Width="76" Height="25" Cursor="Hand" HorizontalAlignment="Center" Command="{Binding AccountSms.CmdSmsRequestCommand}">
