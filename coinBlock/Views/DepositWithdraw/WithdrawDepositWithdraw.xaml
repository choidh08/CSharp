                                                                                </Trigger>
                                                                            </ControlTemplate.Triggers>
                                                                        </ControlTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </Style>
                                                        </Button.Style>
                                                    </Button>
                                                </StackPanel>
                                                <StackPanel VerticalAlignment="Center" Visibility="{Binding OtpComplete}">
                                                    <TextBlock Text="{x:Static localization:Resource.IP_Registration_4_11}" VerticalAlignment="Center" HorizontalAlignment="Right" FontSize="12" Foreground="#333" Margin="0,0,5,0"/>
                                                </StackPanel>
                                            </Grid>
                                        </Border>

                                        <Border Grid.Row="5" Grid.Column="0" Style="{StaticResource Border_Line_2_R}">
                                            <TextBlock Text="{x:Static localization:Resource.WithdrawDepositWithdraw_1_16}" FontSize="12" Foreground="#666" VerticalAlignment="Center" Margin="5,0,0,0"/>
                                        </Border>
                                        <Border Grid.Row="5" Grid.Column="1" Style="{StaticResource Border_Line_1_R}">
                                            <TextBlock Text="{Binding accountHolder}" FontSize="12" Foreground="#333" VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0,0,12,0"/>
                                        </Border>
                                        <Border Grid.Row="5"  Grid.Column="2"  Style="{StaticResource Border_Line_2_R}" Visibility="{Binding AccountSms.SmsAll}">
                                            <TextBlock Text="{x:Static localization:Resource.WithdrawDepositWithdraw_1_42}" FontSize="12" Foreground="#666" VerticalAlignment="Center" Margin="5,0,0,0" />
                                        </Border>
                                        <Border Grid.Row="5" Grid.Column="3" Style="{StaticResource Border_Line_1}" Visibility="{Binding AccountSms.SmsAll}">
