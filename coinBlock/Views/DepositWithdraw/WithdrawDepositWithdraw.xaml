                                                    <Button Width="65" Height="26" Cursor="Hand" HorizontalAlignment="Center" Command="{Binding CmdEmailConfirmCommand}" Margin="0,0,5,0">
                                                        <Button.Style>
                                                            <Style TargetType="{x:Type Button}">
                                                                <Setter Property="Template">
                                                                    <Setter.Value>
                                                                        <ControlTemplate TargetType="{x:Type Button}">
                                                                            <Grid>
                                                                                <Image Name="btnimg" Source="{Binding btn_cert_confirm, TargetNullValue={x:Null}}"/>
                                                                            </Grid>
                                                                            <ControlTemplate.Triggers>
                                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                                    <Setter Property="Source" TargetName="btnimg" Value="{Binding btn_cert_confirm_on}"/>
                                                                                </Trigger>
                                                                            </ControlTemplate.Triggers>
                                                                        </ControlTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </Style>
                                                        </Button.Style>
                                                    </Button>
                                                    <Button Width="65" Height="26" Cursor="Hand" HorizontalAlignment="Center" Command="{Binding CmdEmailResendCommand}" Margin="0,0,5,0">
                                                        <Button.Style>
                                                            <Style TargetType="{x:Type Button}">
                                                                <Setter Property="Template">
                                                                    <Setter.Value>
                                                                        <ControlTemplate TargetType="{x:Type Button}">
                                                                            <Grid>
                                                                                <Image Name="btnimg" Source="{Binding btn_cert_resend, TargetNullValue={x:Null}}"/>
                                                                            </Grid>
                                                                            <ControlTemplate.Triggers>
                                                                                <Trigger Property="IsMouseOver" Value="True">
                                                                                    <Setter Property="Source" TargetName="btnimg" Value="{Binding btn_cert_resend_on}"/>
                                                                                </Trigger>
                                                                            </ControlTemplate.Triggers>
                                                                        </ControlTemplate>
                                                                    </Setter.Value>
                                                                </Setter>
                                                            </Style>
                                                        </Button.Style>
                                                    </Button>
                                                    <Button Width="65" Height="26" Cursor="Hand" HorizontalAlignment="Center" Command="{Binding CmdEmailCancelCommand}">
                                                        <Button.Style>
                                                            <Style TargetType="{x:Type Button}">
                                                                <Setter Property="Template">
                                                                    <Setter.Value>
                                                                        <ControlTemplate TargetType="{x:Type Button}">
                                                                            <Grid>
